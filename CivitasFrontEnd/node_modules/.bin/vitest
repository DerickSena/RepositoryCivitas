#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dericksena/RepositoryCivitas/CivitasFrontEnd/node_modules/.pnpm/vitest@3.2.2_@types+node@22.15.29_jsdom@26.1.0_sugarss@4.0.1_postcss@8.5.4_/node_modules/vitest/node_modules:/home/dericksena/RepositoryCivitas/CivitasFrontEnd/node_modules/.pnpm/vitest@3.2.2_@types+node@22.15.29_jsdom@26.1.0_sugarss@4.0.1_postcss@8.5.4_/node_modules:/home/dericksena/RepositoryCivitas/CivitasFrontEnd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dericksena/RepositoryCivitas/CivitasFrontEnd/node_modules/.pnpm/vitest@3.2.2_@types+node@22.15.29_jsdom@26.1.0_sugarss@4.0.1_postcss@8.5.4_/node_modules/vitest/node_modules:/home/dericksena/RepositoryCivitas/CivitasFrontEnd/node_modules/.pnpm/vitest@3.2.2_@types+node@22.15.29_jsdom@26.1.0_sugarss@4.0.1_postcss@8.5.4_/node_modules:/home/dericksena/RepositoryCivitas/CivitasFrontEnd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
