#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dericksena/RepositoryCivitas/CivitasBackEnd/node_modules/.pnpm/lcov-parse@1.0.0/node_modules/lcov-parse/bin/node_modules:/home/dericksena/RepositoryCivitas/CivitasBackEnd/node_modules/.pnpm/lcov-parse@1.0.0/node_modules/lcov-parse/node_modules:/home/dericksena/RepositoryCivitas/CivitasBackEnd/node_modules/.pnpm/lcov-parse@1.0.0/node_modules:/home/dericksena/RepositoryCivitas/CivitasBackEnd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dericksena/RepositoryCivitas/CivitasBackEnd/node_modules/.pnpm/lcov-parse@1.0.0/node_modules/lcov-parse/bin/node_modules:/home/dericksena/RepositoryCivitas/CivitasBackEnd/node_modules/.pnpm/lcov-parse@1.0.0/node_modules/lcov-parse/node_modules:/home/dericksena/RepositoryCivitas/CivitasBackEnd/node_modules/.pnpm/lcov-parse@1.0.0/node_modules:/home/dericksena/RepositoryCivitas/CivitasBackEnd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lcov-parse/bin/cli.js" "$@"
else
  exec node  "$basedir/../lcov-parse/bin/cli.js" "$@"
fi
